@model IEnumerable<FWDBApp.Models.Framework>

@{
    Layout = null;
}

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FrameworkName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserCount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SupportedBy)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FrameworkName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserCount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SupportedBy)
        </td>
    </tr>
}
</table>
